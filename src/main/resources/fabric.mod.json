{
    "schemaVersion": 1,
    "id": "modget",
    "version": "${version}",
    "name": "Modget",
    "description": "Minecraft Mod Package Manager! Can install mods via commands, search for updates and more.",
    "authors": [
        "NebelNidas"
    ],
    "contact": {
        "homepage": "https://github.com/ReviversMC/modget-minecraft",
        "sources": "https://github.com/ReviversMC/modget-minecraft",
        "issues": "https://github.com/ReviversMC/modget-minecraft/issues"
    },
    "icon": "assets/modget/icon.png",
    "license": "LGPL-3.0-or-later",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.github.nebelnidas.modget.modget_minecraft.Modget"
        ]
    },
    "depends": {
        "fabricloader": ">=0.7.4",
        "fabric-api-base": "*",
        "fabric-command-api-v1": ">=1.1.0+f9b3753b7d",
        "fabric-networking-api-v1": "*",
        "minecraft": ">=1.17"
    },
    "recommends": {
        "server_translations_api": "*"
    }
}
